
<section class="headImg">

</section>

<div id="home">
  <div class="container">
    <div class="row">

      {{#each list}}

      <div class="col m3">
        <div class="card">
          <div class="card-image">
            <img src="https://images.unsplash.com/photo-1569055240014-3a2a63b01e97?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=2550&q=80">
            <span class="card-title">{{title}}</span>
          </div>
          <div class="card-content">
            <p>{{description}}</p>
          </div>
          <div class="card-action">
            <a href="/viewpost/{{id}}">This is a link</a>
          </div>
        </div>
      </div>

      {{/each}}

    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>